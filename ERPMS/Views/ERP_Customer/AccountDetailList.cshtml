
@{
    Layout = null;
}

<!-- 客户对账单清单Html-->
<div style="padding:5px 10px;height:88%;">
    <div>
        <a href="#" class="easyui-linkbutton" plain="true" id="accountDetailList_search"> <i class="fa fa-search fa-lg"></i>&nbsp;查找</a>
        <a href="#" class="easyui-linkbutton" plain="true" id="accountDetailList_excel"> <i class="fa fa-file-excel-o fa-lg"></i>&nbsp;excel</a>
        <a href="#" class="easyui-linkbutton" plain="true" id="accountDetailList_print"> <i class="fa fa-print fa-lg"></i>&nbsp;打印</a>
        <input type="hidden" id="quotation_orderNo" />
    </div>
    <div class="column"><span class="current">客户对账单清单</span></div>
    <div id="accountDetailList_datagrid"> </div>
    <div id="accountDetailList_dialog" class="easyui-dialog" title="查询条件" data-options="modal:true,closed:true,toolbar:'#accountDetailList_toolbar_buttons'" style="width:600px;height:210px;padding:10px;">
        <form id="accountDetailList_form">
            <table class="kv-table">
                <tbody>
                    <tr>
                        <td class="kv-label">报价单号</td>
                        <td class="kv-content"><input class="easyui-combobox" name="orderNo" id="accountDetailList_accountNo" /></td>
                        <td class="kv-label">客户名称</td>
                        <td class="kv-content"><input class="easyui-combobox" name="customerNo" id="accountDetailList_customerNo" /></td>

                    </tr>
                    <tr>
                        <td class="kv-label">对账开始日期</td>
                        <td class="kv-content"><input class="easyui-datebox" name="zhidanStart" /></td>
                        <td class="kv-label">对账结束日期</td>
                        <td class="kv-content"><input class="easyui-datebox" name="zhidanEnd" id="accountDetailList_zhidanend" /></td>
                    </tr>

                </tbody>
            </table>
        </form>
    </div>
    <div id="accountDetailList_toolbar_buttons">
        <table cellpadding="0" cellspacing="0" style="width:100%">
            <tr>
                <td>
                    <a href="#" class="easyui-linkbutton" plain="true" id="accountDetailList_go"><i class="fa fa-check fa-lg"></i>&nbsp;确定</a>
                    <a href="#" class="easyui-linkbutton" plain="true" id="accountDetailList_cancel"><i class="fa fa-times fa-lg"></i>&nbsp;取消</a>
                </td>
            </tr>
        </table>
    </div>
    <iframe src="" frameborder="0" id="accountDetailListIframe" style="visibility:hidden;height:0px;width:0px;"></iframe>
</div>
<script>
    erpRequire(['customer/accountDetailList'])
</script>
