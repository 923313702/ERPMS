
@{
    Layout = null;
}

<!-- 客户日志记录表Html-->
<div style="padding: 10px 5px 5px 10px;height:97%;">
    <div class="column"><span class="current">客户日志记录表</span></div>
    <div style="height:90%;width:100%">
        <div id="ERP_customerLog"></div>
        <div id="ERP_customerLog_tb">
            <a href="#" class="easyui-linkbutton" plain="true" id="customerLog_tb_add"> <i class="fa fa-plus-square fa-lg"></i>&nbsp;添加</a>
            <a href="#" class="easyui-linkbutton" plain="true" id="customerLog_tb_edit"> <i class="fa fa-pencil-square fa-lg"></i>&nbsp;修改</a>
            <a href="#" class="easyui-linkbutton" plain="true" id="customerLog_tb_remove"> <i class="fa fa-trash fa-lg"></i>&nbsp;删除</a>
            <a href="#" class="easyui-linkbutton" plain="true" id="customerLog_tb_save" flag=""> <i class="fa fa-save fa-lg"></i>&nbsp;保存</a>
            <a href="#" class="easyui-linkbutton" plain="true" id="customerLog_tb_revort"> <i class="fa fa-reply-all fa-lg"></i>&nbsp;撤销</a>
            <a href="#" class="easyui-linkbutton" plain="true" id="customerLog_tb_check"> <i class="fa fa-check-circle fa-lg"></i>&nbsp;确认</a>
            <a href="#" class="easyui-linkbutton" plain="true" id="customerLog_tb_excel"> <i class="fa fa-file-excel-o fa-lg"></i>&nbsp;excel</a>
            @*<a href="#" class="easyui-linkbutton" plain="true"> <i class="fa fa-download fa-lg"></i>&nbsp;下载</a>*@
            <a href="#" class="easyui-linkbutton" plain="true" id="customerLog_tb_print"> <i class="fa fa-print fa-lg"></i>&nbsp;打印</a>
          
        </div>

    </div>

    <iframe src="" frameborder="0" id="customerlogList" name="printIframe" style="visibility:hidden;height:0px;"></iframe>

</div>
<script>
    erpRequire(['customer/customerLog'])
</script>