
@{
    Layout = null;
}

<style>
    #quotation2_form .combo, #quotation2_form .textbox, #quotation2_form .datebox, #quotation2_form .validatebox {
        height: 20px;
        width: 120px !important;
    }
</style>

<div style="padding: 10px 5px 5px 10px;">


    <div class="mytools">
      
      
        <a href="#" class="easyui-linkbutton" plain="true" id="quotation2_pre"><i class="fa fa-step-backward fa-lg"></i>&nbsp;前翻</a>
        <a href="#" class="easyui-linkbutton" plain="true" id="quotation2_next"><i class="fa fa-step-forward fa-lg"></i>&nbsp;后翻</a>
        <input type="hidden" id="quotation2_page" value="0" readonly />
        <input type="hidden" id="quotation2_total" value="-1" readonly />
        <a href="#" class="easyui-linkbutton" plain="true" id="quotation2_print"> <i class="fa fa-print fa-lg"></i>&nbsp;打印</a>
    </div>

    <div style="height:50px;"></div>
    <!--startprint-->
    <div class="content" id="quotation2_printhtml">

        <div class="column"><span class="current">报价单</span> &nbsp;&nbsp;&nbsp;<span id="quotation2_orderNo_text" style="color:red;"></span></div>
        <form id="quotation2_form" class="myform">
            <table class="kv-table">
                <tbody>
                    <tr>
                        <td class="kv-label">印品名称</td>
                        <td class="kv-content">
                            <input class="easyui-textbox" required name="订单名称" />
                            <input type="hidden" readonly id="quotation2_orderNo" name="订单号" />
                        </td>
                        <td class="kv-label">客户名称</td>
                        <td class="kv-content"><input class="easyui-combobox" id="quotation2_customer" required name="客户编码" /></td>
                        <td class="kv-label">联系方式</td>
                        <td class="kv-content"><input class="easyui-textbox" name="联系方式" /></td>
                        <td class="kv-label">是否开票</td>
                        <td class="kv-content"><input type="checkbox" value="1" name="是否开票" />是否开票</td>
                    </tr>
                    <tr>
                        <td class="kv-label">成品数量</td>
                        <td class="kv-content"><input class="easyui-numberbox" name="成品数量" required id="quotation2_number" /></td>
                        <td class="kv-label">计量单位</td>
                        <td class="kv-content"><input class="easyui-combobox" name="计量单位" id="quitation_unit" /></td>
                        <td class="kv-label">业务员</td>
                        <td class="kv-content"><input class="easyui-combobox" id="quotation2_saleMan" name="业务员编码" /></td>
                        <td class="kv-label">印品类别</td>
                        <td class="kv-content"><input class="easyui-combobox " id="quotation2_category" name="印品类别编码" /></td>
                    </tr>
                    <tr>
                        <td class="kv-label">开本</td>
                        <td class="kv-content"><input class="easyui-textbox"/></td>
                        <td class="kv-label">装订方法</td>
                        <td class="kv-content"><input class="easyui-textbox" /></td>
                        <td class="kv-label">成品尺寸</td>
                        <td class="kv-content"><input class="easyui-textbox" name="成品尺寸"/></td>
                        <td class="kv-label">交货日期</td>
                        <td class="kv-content"><input class="easyui-textbox"/></td>
                    </tr>

                    <tr>
                        <td class="kv-label">客户要求</td>
                        <td class="kv-content" colspan="7"><input class="easyui-textbox" data-options="multiline:true" style="min-width:700px;height:50px;" name="报价要求" /></td>
                    </tr>
                    <tr>
                        <td class="kv-label">制单人</td>
                        <td class="kv-content"><input class="easyui-combobox" id="quotation2_person" readonly name="制单人编码" /></td>
                        <td class="kv-label">制单日期</td>
                        <td class="kv-content"><input class="easyui-datebox" required name="制单日期" /></td>
                        <td class="kv-label">审核人</td>
                        <td class="kv-content"><input class="easyui-combobox" id="quotation2_auditorPerson" name="审核人编码" readonly /></td>
                        <td class="kv-label">审核日期</td>
                        <td class="kv-content"><input class="easyui-datebox" id="quotation2_auditorTime" name="审核日期" readonly /></td>
                    </tr>
                </tbody>
            </table>
        </form>
        <div class="column"><span class="current">报价纸张</span></div>
   <div style="height:200px">
       <div id="quotation2_detail"></div>
   </div>
     

        <div class="column"><span class="current">报价项目</span></div>
        <div style="height:200px;">
            <div id="quotation2_price"></div>
        </div>
       
    </div>
    <!--endprint-->
    <iframe id="quotation_printf" src="" width="0" height="0" frameborder="0">


    </iframe>
</div>
<script>
    erpRequire(['quotation/quotatioanBill'])
</script>