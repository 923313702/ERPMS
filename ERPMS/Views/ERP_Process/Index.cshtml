
@{
    Layout = null;
}
<!-- 工艺编码表Html-->
<div style="padding: 10px 5px 5px 10px;height:97%;">
    <div class="column"><span class="current">工艺编码表</span></div>
    <div style="height:90%;width:100%" >
        <div id="ERP_process"></div>
        <div id="ERP_process_tb">
            <a href="#" class="easyui-linkbutton" plain="true" id="process_tb_add"> <i class="fa fa-plus-square fa-lg"></i>&nbsp;添加</a>
            <a href="#" class="easyui-linkbutton" plain="true" id="process_tb_edit"> <i class="fa fa-pencil-square fa-lg"></i>&nbsp;修改</a>
            <a href="#" class="easyui-linkbutton" plain="true" id="process_tb_remove"> <i class="fa fa-trash fa-lg"></i>&nbsp;删除</a>
            <a href="#" class="easyui-linkbutton" plain="true" id="process_tb_save" flag=""> <i class="fa fa-save fa-lg"></i>&nbsp;保存</a>
            <a href="#" class="easyui-linkbutton" plain="true" id="process_tb_revort"> <i class="fa fa-reply-all fa-lg"></i>&nbsp;撤销</a>
            @*<a href="#" class="easyui-linkbutton" plain="true"> <i class="fa fa-refresh fa-lg"></i>&nbsp;刷新</a>*@
            <a href="#" class="easyui-linkbutton" plain="true" id="process_tb_excel"> <i class="fa fa-file-excel-o fa-lg"></i>&nbsp;excel</a>
            @*<a href="#" class="easyui-linkbutton" plain="true"> <i class="fa fa-download fa-lg"></i>&nbsp;下载</a>*@
            <a href="#" class="easyui-linkbutton" plain="true" id="process_tb_print"  > <i class="fa fa-print fa-lg"></i>&nbsp;打印</a>
            <input class="easyui-combobox" id="process_category" data-options="prompt:'请选择工艺类别'" />
        </div>

    </div>

        <iframe src="" frameborder="0" id="printIframe" name="printIframe" style="visibility:hidden;height:0px;"></iframe>

</div>
<script>
    erpRequire(['Basics/process'])
</script>
